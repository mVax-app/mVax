    <string name="role_info_title">Roles</string>
    <string name="role_info_desc">mVax supports two different roles: admin and user.\n\n
                                  An admin can:\n
                                    &#8226; view all patients and vaccine records\n
                                    &#8226; create new patients and vaccine records\n
                                    &#8226; edit or delete existing data\n
                                    &#8226; approve new users\n
                                    &#8226; change the role of or delete existing users\n
                                  \n
                                  A user can:\n
                                    &#8226; view all patients and vaccine records\n
                                    &#8226; create new users and vaccine records\n
                                    &#8226; edit or delete existing data</string>

    <string name="approve_user_modal_title">Approve user request</string>

    <string name="approve_user_subtitle">You\'ve chosen to confirm %1$s\'s request for an mVax account with %2$s privileges. Tap Confirm to activate the new account and notify the user that they can now access mVax.</string>

    <string name="welcome_email_subject">mVax account approved</string>
    <string name="welcome_email_body">Hello %1$s,\n\nYour request for an mVax account has been approved. An administrator has activated your account with %2$s privileges, and you can now use your email and password to log into mVax.\n\n\nThanks,\n\nThe mVax team</string>

    <string name="confirm_email_subject">mVax account request received</string>
    <string name="confirm_email_body">Hello %1$s,\n\nWe have received your request for an mVax account. Please wait for an administrator to approve or deny your request. You will receive a confirmation email when your request has been reviewed.\n\n\nThanks,\n\nThe mVax team</string>

    <string name="deny_email_subject">mVax account request denied</string>
    <string name="deny_email_body">Hello %1$s,\n\nYour request for an mVax account has been denied. Please request another account or contact an administrator.\n\n\nThanks,\n\nThe mVax team</string>

